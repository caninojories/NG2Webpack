{
  "name": "NG2Webpack",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "watch": "tsc -w",
    "start": "concurrently \"npm run build:watch\" \"npm run serve:build\"",
    "serve:build": "npm run lint && npm run build:webpack && http-server dist/",
    "webdriver:update": "webdriver-manager update --standalone false --gecko false",
    "test": "concurrently \"npm run build:watch\" \"karma start config/karma.conf.js\"",
    "test:once": "karma start config/karma.conf.js --single-run",
    "tsc": "tsc",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "start:webpack": "webpack-dev-server --inline --progress --port 8080",
    "build:webpack": "rimraf dist && webpack --config config/webpack.prod.js"
  },
