{
  "name": "NG2Webpack",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "watch": "tsc -w",
    "webdriver:update": "webdriver-manager update --standalone false --gecko false",
    "test": "karma start config/karma.conf.js --single-run",
    "tsc": "tsc",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "start:webpack": "webpack-dev-server --inline --progress --port 8081",
    "node:start:webpack": "webpack --config back/webpack.prod.js --watch & nodemon dist/app.server.js $PORT $LOGS",
    "start:dev": "concurrently \"npm run start:webpack\" \"npm run node:start:webpack\"",
    "build:webpack": "rimraf dist && webpack --config config/webpack.prod.js",
    "node:build:webpack": "concurrently \"webpack --config back/webpack.prod.js\" \"npm run build:webpack\" & nodemon dist/app.server.js",
    "postinstall": "yarn global add karma-spec-reporter karma-chrome-launcher karma-cli karma-jasmine karma-webpack karma-sourcemap-loader ts-node"
  },
