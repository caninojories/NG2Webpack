{
  "name": "NG2Webpack",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "watch": "tsc -w",
    "test": "karma start config/karma.conf.ts --single-run",
    "tsc": "tsc",
    "tslintcheck": "tslint -c tslint.json '**/*.ts' --exclude 'node_modules/**'",
    "start:client:webpack": "webpack-dev-server --inline --progress --port 8081",
    "start:server:webpack": "nodemon back/app.server.js $PORT $LOGS",
    "start:dev": "tsc && concurrently \"npm run start:client:webpack\" \"npm run start:server:webpack\"",
    "build:client:webpack": "rimraf dist && webpack --config config/webpack.prod.js",
    "build:server:webpack": "rimraf build && webpack --config back/webpack.prod.js",
    "start:prod": "tsc && npm run build:client:webpack && npm run build:server:webpack && nodemon build/app.server.js",
    "build:prod": "tsc && npm run build:client:webpack && npm run build:server:webpack",
    "addhooks": "cp hooks/pre-commit .git/hooks && chmod +x .git/hooks/pre-commit",
    "postinstall": "yarn global add karma mocha nodemon && npm run addhooks"
  },
  "keywords": [],
  "author": "Jo-Ries Canino",
  "license": "MIT",
